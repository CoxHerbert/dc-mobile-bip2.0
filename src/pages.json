        "backgroundColor": "#F7F7F7"
    },
    "tabBar": {
        "color": "#AFB8C3",
        "selectedColor": "#666",
        "borderStyle": "white",
        "backgroundColor": "#ffffff",
        "list": [
            {
                "pagePath": "pages/plugin/workflow/pages/workbench/index",
                "iconPath": "/static/images/tabbar/tab1.png",
                "selectedIconPath": "./static/images/tabbar/tab1s.png",
